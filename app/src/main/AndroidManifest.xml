<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trip.andrew.tripplanner" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".trip_select"
            android:label="@string/title_activity_trip_select" >
        </activity>
        <activity
            android:name=".trip_add"
            android:label="@string/title_activity_trip_add" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".trip_select" />
        </activity>
        <activity
            android:name=".trip_details"
            android:label="@string/title_activity_trip_details" >
        </activity>
        <activity
            android:name=".museum_edit"
            android:label="@string/title_activity_museum_edit" >
        </activity>
        <activity
            android:name=".add_event"
            android:label="@string/title_activity_add_event" >
        </activity>
    </application>

</manifest>
